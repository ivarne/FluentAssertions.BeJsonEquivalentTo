<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Version>0.0.1</Version>
    <PackageId>Ivarne.FluentAssertions.JsonEquivalent</PackageId>
    <Authors>@ivarne</Authors>
    <Description>
      Extension for FluentAssertions to compare JSON strings.
    </Description>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageTags>FluentAssertions,JSON,BeJsonEquivalent</PackageTags>
    <RepositoryUrl>https://github.com/ivarne/FluentAssertions.BeJsonEquivalentTo</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>Unlicense</PackageLicenseExpression>

    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    
  </PropertyGroup>

  <ItemGroup>
    <None Include="../README.md" Pack="true" PackagePath="" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FluentAssertions" Version="6.12.0" />
  </ItemGroup>

</Project>
